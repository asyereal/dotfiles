# SSH key for own account setting on github
Host github.com-main
	HostName github.com
	User asyereal
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_rsa.main

# SSH key for files repo on github
Host github.com-files
	HostName github.com
	User asyereal
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_rsa.files

# SSH key for site repo on github
Host github.com-asyereal.github.io
	HostName github.com
	User asyereal
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_rsa.asyereal.github.io

# SSH key for my darknet server
Host 192.168.100.109
	Hostname 192.168.100.109
	User asyereal
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_rsa.server

# SSH key for BlenderCity repo on github
Host github.com-BlenderCity
	HostName github.com
	User asyereal
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_rsa.BlenderCity
